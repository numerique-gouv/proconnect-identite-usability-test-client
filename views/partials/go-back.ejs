<div class="fr-mt-2w">
	<button
		class="fr-btn fr-btn--tertiary fr-icon-arrow-go-back-line fr-btn--icon-left go-back-link"
	>
		retour
	</button>
</div>
<script>
	document.addEventListener(
		"DOMContentLoaded",
		function () {
			var goBackLinks = document.querySelectorAll(".go-back-link");

			function goBack() {
				history.back();
			}

			goBackLinks.forEach(function (goBackLink) {
				goBackLink.addEventListener("click", goBack);
			});
		},
		false
	);
</script>
